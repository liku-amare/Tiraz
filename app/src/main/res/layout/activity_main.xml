<androidx.constraintlayout.widget.ConstraintLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent">

    <ImageView
        android:id="@+id/imageView"
        android:layout_width="238dp"
        android:layout_height="100dp"
        android:layout_marginStart="87dp"
        android:layout_marginTop="53dp"
        android:layout_marginEnd="87dp"
        android:layout_marginBottom="23dp"
        android:contentDescription="@string/logo_title"
        app:layout_constraintBottom_toTopOf="@+id/gridLayout"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/logo2_no_back" />

    <androidx.gridlayout.widget.GridLayout
        android:id="@+id/gridLayout"
        android:layout_width="305dp"
        android:layout_height="447dp"
        app:layout_constraintBottom_toTopOf="@+id/button_settings"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/imageView">

        <LinearLayout
            android:id="@+id/tiraz1_home_button"
            android:layout_width="143dp"
            android:layout_height="194dp"
            android:layout_margin="5dp"
            android:background="@drawable/shape"
            android:theme="@style/ErrorStyle"
            android:orientation="vertical"
            app:layout_column="0"
            app:layout_row="0">

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_gravity="center"
                android:layout_marginTop="10dp"
                android:layout_marginBottom="50dp"
                android:fontFamily="@font/niyala"
                android:text="ጥራዝ አንድ"
                android:textSize="28sp" />

            <TextView
                android:id="@+id/tiraz1_year"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_gravity="center"
                android:layout_marginTop="10dp"
                android:layout_marginBottom="5dp"
                android:fontFamily="@font/niyala"
                android:text="1999 ዓ.ም."
                android:textSize="22sp" />

            <TextView
                android:id="@+id/tiraz1_hymns"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_gravity="center"
                android:layout_marginTop="5dp"
                android:fontFamily="@font/niyala"
                android:text="100 መዝሙራት"
                android:textSize="22sp" />
        </LinearLayout>

        <LinearLayout
            android:id="@+id/tiraz2_home_button"
            android:layout_width="143dp"
            android:layout_height="194dp"
            android:layout_margin="5dp"
            android:background="@drawable/shape"
            android:orientation="vertical"
            android:theme="@style/ErrorStyle"
            app:layout_column="1"
            app:layout_row="0">

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_gravity="center"
                android:layout_marginTop="10dp"
                android:layout_marginBottom="50dp"
                android:fontFamily="@font/niyala"
                android:text="ጥራዝ ሁለት"
                android:textSize="28sp" />

            <TextView
                android:id="@+id/tiraz2_year"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_gravity="center"
                android:layout_marginTop="10dp"
                android:layout_marginBottom="5dp"
                android:fontFamily="@font/niyala"
                android:text="1999 ዓ.ም."
                android:textSize="22sp" />

            <TextView
                android:id="@+id/tiraz2_hymns"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_gravity="center"
                android:layout_marginTop="5dp"
                android:fontFamily="@font/niyala"
                android:text="100 መዝሙራት"
                android:textSize="22sp" />

        </LinearLayout>

        <LinearLayout
            android:id="@+id/tiraz3_home_button"
            android:layout_width="143dp"
            android:layout_height="194dp"
            android:layout_margin="5dp"
            android:background="@drawable/shape"
            android:theme="@style/ErrorStyle"
            android:orientation="vertical"
            app:layout_column="0"
            app:layout_row="1">

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_gravity="center"
                android:layout_marginTop="10dp"
                android:layout_marginBottom="50dp"
                android:fontFamily="@font/niyala"
                android:text="ጥራዝ ሦስት"
                android:textSize="28sp" />

            <TextView
                android:id="@+id/tiraz3_year"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_gravity="center"
                android:layout_marginTop="10dp"
                android:layout_marginBottom="5dp"
                android:fontFamily="@font/niyala"
                android:text="1999 ዓ.ም."
                android:textSize="22sp" />

            <TextView
                android:id="@+id/tiraz3_hymns"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_gravity="center"
                android:layout_marginTop="5dp"
                android:fontFamily="@font/niyala"
                android:text="100 መዝሙራት"
                android:textSize="22sp" />

        </LinearLayout>

        <LinearLayout
            android:id="@+id/tiraz4_home_button"
            android:layout_width="143dp"
            android:layout_height="194dp"
            android:layout_margin="5dp"
            android:background="@drawable/shape"
            android:theme="@style/ErrorStyle"
            android:orientation="vertical"
            app:layout_column="1"
            app:layout_row="1">

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_gravity="center"
                android:layout_marginTop="10dp"
                android:layout_marginBottom="50dp"
                android:fontFamily="@font/niyala"
                android:text="ጥራዝ አራት"
                android:textSize="28sp" />

            <TextView
                android:id="@+id/tiraz4_year"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_gravity="center"
                android:layout_marginTop="10dp"
                android:layout_marginBottom="5dp"
                android:fontFamily="@font/niyala"
                android:text="1999 ዓ.ም."
                android:textSize="22sp" />

            <TextView
                android:id="@+id/tiraz4_hymns"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_gravity="center"
                android:layout_marginTop="5dp"
                android:fontFamily="@font/niyala"
                android:text="100 መዝሙራት"
                android:textSize="22sp" />

        </LinearLayout>

    </androidx.gridlayout.widget.GridLayout>

    <Button
        android:id="@+id/button_settings"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="158dp"
        android:layout_marginTop="14dp"
        android:layout_marginEnd="159dp"
        android:layout_marginBottom="79dp"
        android:background="@drawable/shape"
        android:text="@string/button_setting_title"
        android:fontFamily="@font/niyala"
        android:textSize="22sp"
        android:textColor="@color/text_coffee"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/gridLayout" />

</androidx.constraintlayout.widget.ConstraintLayout>